    "dev": "npm run serve & npm run watch",
    "compile": "node_modules/.bin/browserify ./src/index.js -o dist/gh-events-widget.js -t browserify-css -t [ babelify --presets [ es2015] ] -g uglifyify",
    "distribute": "npm run compile && node bin/distribute"
  },
